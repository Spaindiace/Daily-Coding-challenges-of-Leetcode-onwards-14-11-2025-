// c++ 
class Solution {
public:
    int countPalindromicSubsequence(string s) {
        int n = s.size();
        set<pair<char, char>> res;
        unordered_set<char> left;
        unordered_map<char, int> right;
        for (auto& k : s) {
            right[k]++;
        }
        int cnt = 0;
        for (int i = 0; i < n; i++) {
            right[s[i]]--;
            if (right[s[i]] == 0) {
                right.erase(s[i]);
            }
            for (auto& k : left) {
                if (right.find(k) != right.end()) {
                    res.insert(make_pair(k, s[i]));

                } else {
                    continue;
                }
            }

            left.insert(s[i]);
        }
        return res.size();
    }
};
